@model IEnumerable<Orquesta.Models.getPedidos>

@{
    ViewBag.Title = "Pedidos";
}

<h2>Pedidos</h2>

<p>
    @Html.ActionLink("Nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id_Pedido)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.canal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.usuario)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Fecha_Ingreso)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Comentario)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Id_Pedido)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.canal)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.usuario)
    </td>

    <td>
        @Html.DisplayFor(modelItem => item.Fecha_Ingreso)
    </td>

    <td>
        @Html.DisplayFor(modelItem => item.Comentario)
    </td>
    <td>
        @Html.ActionLink("Editar", "Edit", new { id = item.Id_Pedido }) |
        @Html.ActionLink("Detalles", "Details", new { id = item.Id_Pedido }) |
        @Html.ActionLink("Eliminar", "Delete","getPedidos", new { id = item.Id_Pedido })
    </td>
</tr>
}

</table>
